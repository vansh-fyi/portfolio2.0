{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "useDefineForClassFields": true,
    "types": [
      "vite/client",
      "node",
      "react",
      "react-dom"
    ],
    "moduleDetection": "force",
    "allowImportingTsExtensions": false,
    "erasableSyntaxOnly": true,
    "noUncheckedSideEffectImports": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@trpc/server": [
        "./node_modules/@trpc/server"
      ],
      "zod": [
        "./node_modules/zod"
      ],
      "hono": [
        "./node_modules/hono"
      ],
      "hono/*": [
        "./node_modules/hono/*"
      ],
      "@hono/trpc-server": [
        "./node_modules/@hono/trpc-server"
      ],
      "dotenv": [
        "./node_modules/dotenv"
      ],
      "resend": [
        "./node_modules/resend"
      ],
      "@supabase/supabase-js": [
        "./node_modules/@supabase/supabase-js"
      ],
      "ai": [
        "./node_modules/ai"
      ],
      "@ai-sdk/huggingface": [
        "./node_modules/@ai-sdk/huggingface"
      ],
      "@hono/node-server": [
        "./node_modules/@hono/node-server"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/**/__tests__/**",
    "src/**/__mocks__/**",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx"
  ]
}